# Hashmancer Integration Test Configuration
# Copy this file to .env.integration and fill in your actual API keys

# Required API Keys
HASHES_COM_API_KEY=your_hashes_com_api_key_here
VAST_AI_API_KEY=your_vast_ai_api_key_here

# Optional Configuration (with defaults)
VAST_MAX_COST_PER_HOUR=0.50
TEST_DURATION_MINUTES=15
MIN_JOB_RUNTIME_MINUTES=10

# Test Configuration
HASHES_COM_JOB_TYPE=md5
WORKER_TEST_MODE=true

# Server Configuration (usually defaults are fine)
HASHMANCER_SERVER_URL=http://localhost:8080
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging
LOG_LEVEL=INFO
INTEGRATION_TEST_LOG_FILE=/tmp/hashmancer_integration_test.log