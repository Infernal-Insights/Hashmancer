# Environment variables for Opus Integration
# Copy this to .env and add your actual API keys

# Required for Opus integration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Required for GitHub automation  
GITHUB_TOKEN=your_github_token_here
GITHUB_REPOSITORY=your_username/hashmancer

# Optional: Vast.ai integration
VAST_AI_API_KEY=your_vastai_api_key_here

# Optional: Hashes.com integration
HASHES_COM_API_KEY=your_hashes_com_api_key_here

# Development settings
LOG_LEVEL=INFO
REDIS_HOST=localhost
REDIS_PORT=6379